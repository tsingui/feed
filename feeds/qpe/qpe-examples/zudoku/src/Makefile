####### Compiler, tools and options

CC	=	$(TARGET_CROSS)gcc
CXX	=	$(TARGET_CROSS)g++
CFLAGS	=	-pipe $(TARGET_CFLAGS) -Wall -W -O2 -DQTOPIA_1_5 -DNO_DEBUG
CXXFLAGS=	-pipe $(TARGET_CFLAGS) -DQWS -fno-exceptions -fno-rtti -Wall -W -O2 -DQTOPIA_1_5 -DNO_DEBUG
INCPATH	=	-I. -I$(QTDIR)/include
LINK	=	$(TARGET_CROSS)g++
LFLAGS	=	$(TARGET_LDFLAGS)
LIBS	=	$(SUBLIBS) -L$(QTDIR)/lib -lm $(TARGET_LIBS) -lqpe -lqte
MOC	=	$(QTDIR)/bin/moc
UIC	=	$(QTDIR)/bin/uic

TAR	=	tar -cf
GZIP	=	gzip -9f

####### Files

