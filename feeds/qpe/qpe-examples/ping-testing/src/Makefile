####### Compiler, tools and options

CC	=	$(TARGET_CROSS)gcc
CXX	=	$(TARGET_CROSS)g++
CFLAGS	=	-pipe $(TARGET_CFLAGS) -Wall -W -O2 -DNO_DEBUG
CXXFLAGS=	-pipe $(TARGET_CFLAGS) -DQWS -fno-exceptions -fno-rtti -Wall -W -O2 -DNO_DEBUG
INCPATH	=	-I$(QTDIR)/include
LINK	=	$(TARGET_CROSS)g++
LFLAGS	=	$(TARGET_LDFLAGS)
LIBS	=	$(SUBLIBS) -L$(QTDIR)/lib -lm $(TARGET_LIBS) -lqte -lqpe
MOC	=	$(QTDIR)/bin/moc
UIC	=	$(QTDIR)/bin/uic

TAR	=	tar -cf
GZIP	=	gzip -9f

####### Files

HEADERS =	qtopia-ping.h
SOURCES =	main.cpp \
		qtopia-ping.cpp
OBJECTS =	main.o \
		qtopia-ping.o \
		qtopia-ping-base.o
INTERFACES =	qtopia-ping-base.ui
UICDECLS =	qtopia-ping-base.h
UICIMPLS =	qtopia-ping-base.cpp
SRCMOC	=	moc_qtopia-ping.cpp \
		moc_qtopia-ping-base.cpp
OBJMOC	=	moc_qtopia-ping.o \
		moc_qtopia-ping-base.o
DIST	=	
TARGET	=	ping-testing
INTERFACE_DECL_PATH = .

####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .C .c

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules


all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

moc: $(SRCMOC)

tmake: Makefile

Makefile: ping-testing.pro
	tmake ping-testing.pro -o Makefile

dist:
	$(TAR) ping-testing.tar ping-testing.pro $(SOURCES) $(HEADERS) $(INTERFACES) $(DIST)
	$(GZIP) ping-testing.tar

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC) $(UICIMPLS) $(UICDECLS) $(TARGET)
	-rm -f *~ core

