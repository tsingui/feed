###### Combined headers


####### Compile

main.o: main.cpp \
		qtopia-ping.h \
		qtopia-ping-base.h

qtopia-ping.o: qtopia-ping.cpp \
		qtopia-ping.h \
		qtopia-ping-base.h

qtopia-ping-base.h: qtopia-ping-base.ui
	$(UIC) qtopia-ping-base.ui -o $(INTERFACE_DECL_PATH)/qtopia-ping-base.h

qtopia-ping-base.cpp: qtopia-ping-base.ui
	$(UIC) qtopia-ping-base.ui -i qtopia-ping-base.h -o qtopia-ping-base.cpp

qtopia-ping-base.o: qtopia-ping-base.cpp \
		qtopia-ping-base.h \
		qtopia-ping-base.ui

moc_qtopia-ping.o: moc_qtopia-ping.cpp \
		qtopia-ping.h \
		qtopia-ping-base.h

moc_qtopia-ping-base.o: moc_qtopia-ping-base.cpp \
		qtopia-ping-base.h

moc_qtopia-ping.cpp: qtopia-ping.h
	$(MOC) qtopia-ping.h -o moc_qtopia-ping.cpp

moc_qtopia-ping-base.cpp: qtopia-ping-base.h
	$(MOC) qtopia-ping-base.h -o moc_qtopia-ping-base.cpp

