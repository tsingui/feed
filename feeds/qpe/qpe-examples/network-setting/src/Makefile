####### Compile

main.o: main.cpp \
		network-setting.h \
		network-setting-base.h

network-setting.o: network-setting.cpp \
		network-setting.h \
		network-setting-base.h

network-setting-base.h: network-setting-base.ui
	$(UIC) network-setting-base.ui -o $(INTERFACE_DECL_PATH)/network-setting-base.h

network-setting-base.cpp: network-setting-base.ui
	$(UIC) network-setting-base.ui -i network-setting-base.h -o network-setting-base.cpp

network-setting-base.o: network-setting-base.cpp \
		network-setting-base.h \
		network-setting-base.ui

moc_network-setting.o: moc_network-setting.cpp \
		network-setting.h \
		network-setting-base.h

moc_network-setting-base.o: moc_network-setting-base.cpp \
		network-setting-base.h

moc_network-setting.cpp: network-setting.h
	$(MOC) network-setting.h -o moc_network-setting.cpp

moc_network-setting-base.cpp: network-setting-base.h
	$(MOC) network-setting-base.h -o moc_network-setting-base.cpp

